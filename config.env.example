# Supabase Configuration
# Copy this file to .env and fill in your actual values

# Your Supabase project URL (found in your project settings)
EXPO_PUBLIC_SUPABASE_URL=your_supabase_project_url_here

# Your Supabase anonymous/public key (found in your project settings)
EXPO_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Optional: Supabase service role key (for admin operations - keep secure)
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key_here

# Optional: Database connection string (if needed)
DATABASE_URL=your_database_connection_string_here

# App Configuration
APP_NAME=Hospital Queue
APP_VERSION=1.0.0
APP_ENVIRONMENT=development

# Note: Variables prefixed with EXPO_PUBLIC_ are accessible in your React Native app
# Variables without EXPO_PUBLIC_ are only accessible on the server side
